<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="org.osoro.project10.MainActivity">

    <ImageButton
        android:id="@+id/tap"
        android:layout_width="88dp"
        android:layout_height="42dp"
        android:background="@drawable/joy0"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/sharp"
        app:layout_constraintLeft_toRightOf="@+id/call"
        android:layout_marginLeft="32dp" />

    <ImageButton
        android:id="@+id/star"
        android:layout_width="88dp"
        android:layout_height="42dp"
        android:background="@drawable/joy0"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/button7"
        android:layout_marginLeft="32dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <ImageButton
        android:id="@+id/sharp"
        android:layout_width="88dp"
        android:layout_height="42dp"
        android:background="@drawable/joy0"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/button9"
        app:layout_constraintLeft_toRightOf="@+id/button0"
        android:layout_marginLeft="32dp" />

    <ImageButton
        android:id="@+id/erase"
        android:layout_width="88dp"
        android:layout_height="42dp"
        android:background="@drawable/joy0"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/star"
        android:layout_marginLeft="32dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <ImageButton
        android:id="@+id/call"
        android:layout_width="88dp"
        android:layout_height="42dp"
        android:background="@drawable/joy0"
        app:layout_constraintLeft_toRightOf="@+id/erase"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/button0" />

    <Button
        android:id="@+id/button0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/button8"
        app:layout_constraintLeft_toRightOf="@+id/star"
        android:layout_marginLeft="32dp" />

    <Button
        android:id="@+id/button1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1"
        android:layout_marginLeft="32dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="16dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="2"
        app:layout_constraintLeft_toRightOf="@+id/button1"
        android:layout_marginLeft="32dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="16dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="3"
        app:layout_constraintLeft_toRightOf="@+id/button2"
        android:layout_marginLeft="32dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="16dp" />

    <Button
        android:id="@+id/button4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="4"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/button1"
        android:layout_marginLeft="32dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <Button
        android:id="@+id/button5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="5"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/button2"
        app:layout_constraintLeft_toRightOf="@+id/button4"
        android:layout_marginLeft="32dp" />

    <Button
        android:id="@+id/button6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="6"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/button3"
        app:layout_constraintLeft_toRightOf="@+id/button5"
        android:layout_marginLeft="32dp" />

    <Button
        android:id="@+id/button7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="7"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/button4"
        android:layout_marginLeft="32dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <Button
        android:id="@+id/button8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="8"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/button5"
        app:layout_constraintLeft_toRightOf="@+id/button7"
        android:layout_marginLeft="32dp" />

    <Button
        android:id="@+id/button9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="9"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/button6"
        app:layout_constraintLeft_toRightOf="@+id/button8"
        android:layout_marginLeft="32dp" />

    <EditText
        android:id="@+id/editText"
        android:layout_width="249dp"
        android:layout_height="58dp"
        android:ems="14"
        android:inputType="number"
        android:text="Tel: "
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.504"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/call" />
</android.support.constraint.ConstraintLayout>